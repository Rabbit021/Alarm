<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="Alarm.EditoTool"
	x:Name="UserControl"
	d:DesignWidth="1060" d:DesignHeight="39.251">

	<Grid x:Name="LayoutRoot">
		<Grid x:Name="footer" Background="Gray" >
			<Button x:Name="Add" Content="添加" Click="Add_Click" HorizontalAlignment="Left" Style="{DynamicResource EditBtn}" Command="{Binding AddTaskCommand}"/>
			<Button x:Name="Add_Copy" Content="编辑" Click="Add_Click" HorizontalAlignment="Center" Style="{DynamicResource EditBtn}" Command="{Binding EditTaskCommand}"/>
			<Button x:Name="Add_Copy1" Content="删除" Click="Add_Click" HorizontalAlignment="Right" Style="{DynamicResource EditBtn}" Command="{Binding DelTaskCommand}"/>
		</Grid>
	</Grid>
</UserControl>